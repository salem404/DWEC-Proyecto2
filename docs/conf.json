{
  "plugins": ["node_modules/jsdoc-vuejs"],
  "recurseDepth": 10,
  "source": {
    "include": ["src/", "README.md"],
    "includePattern": "\\.(vue|js)$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
